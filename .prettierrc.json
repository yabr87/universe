{
  "printWidth": 80,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "overrides": [
    {
      "files": "src/**/*.svg",
      "options": {
        "parser": "html",
        "printWidth": 12000
      }
    }
  ]
}
